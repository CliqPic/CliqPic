{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "server",
      "image": "402239435993.dkr.ecr.us-east-1.amazonaws.com/clients/cliq-pic:0.0.54",
      "memoryReservation": "128",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "3000"
        }
      ]
    },
    {
      "name": "sidekiq-scraper",
      "image": "402239435993.dkr.ecr.us-east-1.amazonaws.com/clients/cliq-pic:0.0.54",
      "memoryReservation": "256",
      "command": ["bundle", "exec", "sidekiq", "-q", "scraper", "-c", "5" ]
    },
    {
      "name": "sidekiq-processor",
      "image": "402239435993.dkr.ecr.us-east-1.amazonaws.com/clients/cliq-pic:0.0.54",
      "memoryReservation": "256",
      "command": ["bundle", "exec", "sidekiq", "-q", "processor", "-c", "10"]
    },
    {
      "name": "sidekiq-filter",
      "image": "402239435993.dkr.ecr.us-east-1.amazonaws.com/clients/cliq-pic:0.0.54",
      "memoryReservation": "128",
      "command": ["bundle", "exec", "sidekiq", "-q", "filter", "-c", "5"]
    }
  ]
}
